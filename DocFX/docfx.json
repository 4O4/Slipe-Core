{
  "metadata": [
    {
      "src": [
        {
          "src": "../Slipe/Core/Source/SlipeShared/",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api/shared"
    },
    {
      "src": [
        {
          "src": "../Slipe/Core/Source/",
          "files": [
            "SlipeServer/**.csproj",
            "SlipeSql/**.csproj"
          ]
        }
      ],
      "dest": "api/server"
    },
    {
      "src": [
        {
          "src": "../Slipe/Core/Source/SlipeClient/",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api/client"
    },
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/shared/**.yml",
          "api/server/**.yml",
          "api/client/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}